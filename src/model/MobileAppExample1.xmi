<?xml version="1.0" encoding="UTF-8"?>
<rnAppGen:MobileApp
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:rnAppGen="http://rnappgen"
    xsi:schemaLocation="http://rnappgen ../metamodel/rn_design.ecore"
    name="Aplicacion ejemplo 1">
  <navigator>
    <screens
        title="Primera pantalla">
      <nbComponents
          xsi:type="rnAppGen:Text"
          flexVerticalPosition="1"
          flexVerticalSize="2.0"
          flexHorizontalPosition="1"
          flexHorizontalSize="4.0"
          margin="10"
          text="Esto es texto de ejemplo, lo que aparece arriba del todo de la pantalla"/>
      <nbComponents
          xsi:type="rnAppGen:Card"
          flexVerticalPosition="2"
          flexVerticalSize="2.0"
          flexHorizontalPosition="1"
          flexHorizontalSize="4.0"
          header="Carta de ejemplo"
          body="Otro componente de ejemplo"/>
      <nbComponents
          xsi:type="rnAppGen:InputText"
          flexVerticalPosition="3"
          flexVerticalSize="2.0"
          flexHorizontalPosition="1"
          flexHorizontalSize="3.0"
          margin="10"
          type="underlined"
          placeholder="Enter text"/>
      <nbComponents
          xsi:type="rnAppGen:Button"
          flexVerticalPosition="3"
          flexVerticalSize="2.0"
          flexHorizontalPosition="2"
          flexHorizontalSize="1.0"
          margin="10"
          text="Enviar"/>
      <nbComponents
          xsi:type="rnAppGen:List"
          flexVerticalPosition="4"
          flexVerticalSize="6.0"
          flexHorizontalPosition="1"
          flexHorizontalSize="4.0">
        <items
            text="Elemento numero 1">
          <leftIcon
              name="md-document"
              color="blue"/>
          <rightIcon
              name="arrow-forward"
              color=""/>
        </items>
        <items
            text="Elemento numero 2">
          <leftIcon
              name="md-folder"
              color="red"/>
          <rightIcon
              name="arrow-forward"/>
        </items>
        <items
            text="Elemento numero 3">
          <leftIcon
              name="md-link"
              color="green"/>
          <rightIcon
              name="arrow-forward"/>
        </items>
        <items
            text="Elemento numero 4">
          <leftIcon
              name="md-document"
              color="blue"/>
          <rightIcon
              name="arrow-forward"
              color=""/>
        </items>
        <items
            text="Elemento numero 5">
          <leftIcon
              name="md-folder"
              color="red"/>
          <rightIcon
              name="arrow-forward"/>
        </items>
        <items
            text="Elemento numero 6">
          <leftIcon
              name="md-link"
              color="green"/>
          <rightIcon
              name="arrow-forward"/>
        </items>
      </nbComponents>
    </screens>
  </navigator>
</rnAppGen:MobileApp>

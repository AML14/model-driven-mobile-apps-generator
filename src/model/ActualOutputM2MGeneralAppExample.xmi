      <components xsi:type="rnAppGen:Card" flexVerticalPosition="1" dataSource="//@navigator/@screens.1/@components.0 //@navigator/@screens.0/@components.0 //@navigator/@screens.0/@components.1 //@navigator/@screens.0/@components.2 //@navigator/@screens.0/@components.3" stateID="6"/>
